{
  "name": "express-autodocs",
  "version": "0.0.2-beta.1",
  "description": "A github action which automatically generates documentation for your express APIs",
  "main": "src/index.js",
  "scripts": {
    "test": "mocha || true",
    "dev": "node -r dotenv/config src/index.js ",
    "format": "prettier --write 'helpers/*.js' 'test/*.js'"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Pika1998/express-autodocs.git"
  },
  "keywords": [
    "autodocs",
    "docs",
    "express"
  ],
  "author": "Prafulla Raichurkar",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Pika1998/express-autodocs/issues"
  },
  "homepage": "https://github.com/Pika1998/express-autodocs#readme",
  "dependencies": {
    "@actions/core": "^1.2.4",
    "@actions/github": "^4.0.0",
    "@babel/parser": "^7.11.4",
    "@babel/traverse": "^7.11.0",
    "cli-badges": "^1.0.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "eslint": "^7.10.0",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-config-prettier": "^6.12.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-prettier": "^3.1.4",
    "husky": "^4.3.0",
    "lint-staged": "^10.4.0",
    "mocha": "^8.1.3",
    "prettier": "^2.1.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "prettier --write",
      "git add"
    ]
  }
}